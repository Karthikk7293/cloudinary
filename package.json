{
  "name": "cloudinary-media-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "tsx scripts/seed-admin.ts",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc-files --noEmit"
    ],
    "*.{js,jsx,mjs}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cloudinary": "^2.5.1",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "next": "15.5.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.3.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.5.12",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsc-files": "^1.1.4",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  }
}
